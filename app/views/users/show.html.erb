<div class="row">
  <div class="col-md-4 col-md-offset-2 col-sm-5 col-sm-offset-1">

    <div class="avatar-group">
        <img src="<%= gravatar_for(@user.email, size: 330) %>" class="thumbnail img-rounded media-object">
        <div class="avatar-lower">
            <h3><%= user_display_name @user %></h3>
        </div>
    </div>

  </div>
  <div class="col-md-4 col-sm-5">

  	<h4>Projects</h3>
  	<!-- TODO Insert list of followed projects -->

	</div>
</div>
<div class="row">
  <div class="col-md-4 col-md-offset-2 col-sm-5 col-sm-offset-1">
    
    <%= link_to "Edit", edit_user_registration_path, class: "btn btn-primary", type: "button" if current_user %>

  	<h4>Pair Programming</h4>
    <p>Joined on: <%=date_format(@user.created_at) %> </p>
  	<!-- TODO Insert list of PP youtube or G+ session links -->
    <%if !@youtube_videos.blank?%>
      <%@youtube_videos.each do |link| %>
         <%= link_to(link[:title], link[:url]) %>
      <%end%>
    <%else%>
      <h4><%=user_display_name @user %> has no publicly viewable Youtube videos.</h4>
    <%end%>

  </div>
  <div class="col-md-4 col-sm-5">

  	<!-- TODO Embed youtube player here -->
  	<img src="placeholder" width="300" height="150">

  </div>
</div>
